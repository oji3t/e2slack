{
    "name": "oji3t/e2slack",
    "license": "MIT",
    "type": "library",
    "keywords": ["error", "slack", "notification"],
    "description": "A package to send message to slack when happened exception.",
    "authors": [
        {
            "name": "Takara Oji",
            "email": "kumappler@gmail.com"
        }
    ],
    "require": {
        "maknz/slack": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "vlucas/phpdotenv": "*"
    },
    "autoload": {
        "psr-4": {
            "ExceptionToSlack\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
